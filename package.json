{
  "name": "tth-frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "typecheck": "tsc",
    "test": "jest",
    "test:e2e": "playwright test --config=playwright/playwright.config.ts --project=chromium",
    "test:e2e:firefox": "playwright test --config=playwright/playwright.config.ts --project=firefox",
    "test:e2e:webkit": "playwright test --config=playwright/playwright.config.ts --project=webkit",
    "test:e2e:smoke": "npm run test:e2e -- --grep @smoke",
    "hasura": "env-cmd -f ./hasura/.env hasura --project hasura",
    "hasura:console": "npm run hasura -- console",
    "hasura:squash": "npm run hasura -- migrate squash",
    "hasura:seed:create": "npm run hasura -- seed create",
    "hasura:seed:apply": "npm run hasura -- seed apply",
    "hasura:metadata:apply": "npm run hasura -- metadata apply",
    "hasura:migrate:apply": "npm run hasura -- migrate apply",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/forms": "^0.4.0",
    "@types/amazon-cognito-auth-js": "^1.3.0",
    "@types/chance": "1.1.3",
    "aws-amplify": "^4.3.13",
    "aws-amplify-react": "^5.1.9",
    "chance": "1.1.8",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "deepmerge": "4.2.2",
    "email-validator": "^2.0.4",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "i18next": "^21.6.6",
    "postcss-cli": "^9.1.0",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.25.3",
    "react-i18next": "^11.15.3",
    "react-router-dom": "^6.2.1",
    "react-use": "^17.3.2",
    "swr": "^1.2.1",
    "tailwindcss-ripple": "^0.7.1",
    "vite-plugin-svgr": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@playwright/test": "1.19.1",
    "@storybook/addon-actions": "^6.5.0-alpha.19",
    "@storybook/addon-essentials": "^6.5.0-alpha.19",
    "@storybook/addon-links": "^6.5.0-alpha.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.5.0-alpha.19",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "^17.0.33",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "babel-preset-vite": "1.0.4",
    "env-cmd": "^10.1.0",
    "esbuild": "0.13.8",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "hasura-cli": "^2.1.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.4",
    "nanoid": "^3.2.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "storybook-builder-vite": "^0.1.13",
    "tailwindcss": "^3.0.13",
    "ts-essentials": "9.1.2",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest-setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.svg": "<rootDir>/test/__mocks__/svg.js",
      "^@app/(.*)": "<rootDir>/src/$1",
      "^@test/(.*)": "<rootDir>/test/$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/playwright"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm t"
    ]
  }
}
