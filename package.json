{
  "name": "tth-frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.snap",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.snap",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test --config=playwright/playwright.config.ts --project=chromium",
    "test:e2e:chromium": "playwright test --config=playwright/playwright.config.ts --project=chromium",
    "test:e2e:firefox": "playwright test --config=playwright/playwright.config.ts --project=firefox",
    "test:e2e:webkit": "playwright test --config=playwright/playwright.config.ts --project=webkit",
    "test:e2e:smoke": "npm run test:e2e -- --grep @smoke",
    "test:e2e:data": "npm run test:e2e -- --grep @data",
    "test:e2e:queries": "npm run test:e2e -- --grep @query",
    "test:e2e:report": "playwright show-report",
    "hasura": "env-cmd -f ./hasura/.env -- hasura --project hasura --skip-update-check",
    "hasura:console": "npm run hasura -- console",
    "hasura:squash": "npm run hasura -- migrate squash",
    "hasura:migrate:all:up": "npm run hasura -- migrate apply --database-name default",
    "hasura:migrate:all:down": "npm run hasura -- migrate apply --type down --version 101 --database-name default",
    "hasura:seed:create": "npm run hasura -- seed create",
    "hasura:seed:apply": "npm run hasura -- seed apply --database-name default",
    "hasura:metadata:apply": "npm run hasura -- metadata apply",
    "hasura:migrate:apply": "npm run hasura -- migrate apply",
    "seed:orders": "ts-node ./scripts/seed-orders.ts",
    "prepare": "husky install",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "e2e:graphql-codegen": "graphql-codegen --config playwright/codegen.yml"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@googlemaps/react-wrapper": "^1.1.35",
    "@hookform/resolvers": "^3.1.0",
    "@lexical/react": "^0.9.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.122",
    "@mui/material": "^5.12.0",
    "@mui/utils": "^5.11.2",
    "@mui/x-date-pickers": "^6.0.4",
    "@react-pdf/renderer": "^3.1.8",
    "@sentry/integrations": "^7.48.0",
    "@sentry/react": "^7.48.0",
    "@sentry/tracing": "^7.48.0",
    "@stripe/react-stripe-js": "^2.1.0",
    "@stripe/stripe-js": "^1.52.1",
    "@types/chance": "1.1.3",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-auth": "^1.0.0",
    "amazon-cognito-identity-js": "^6.2.0",
    "autosuggest-highlight": "^3.3.4",
    "aws-amplify": "^5.0.11",
    "chance": "1.1.11",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "deepmerge-ts": "^5.0.0",
    "file-saver": "^2.0.5",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "i18next": "^22.4.10",
    "jszip": "^3.10.1",
    "jwt-decode": "^3.1.2",
    "lexical": "^0.9.0",
    "lodash-es": "^4.17.21",
    "mui-image": "^1.0.7",
    "mui-tel-input": "^3.1.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.1.5",
    "react-router-dom": "^6.10.0",
    "react-use": "^17.4.0",
    "react-youtube": "^10.1.0",
    "react18-input-otp": "^1.1.2",
    "sanitize-html": "^2.9.0",
    "swr": "^2.1.1",
    "urql": "^3.0.3",
    "use-debounce": "^9.0.4",
    "use-query-params": "^2.2.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5",
    "yup": "^1.0.2",
    "yup-password": "0.2.2",
    "yup-phone": "^1.3.2"
