{
  "name": "tth-frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "typecheck": "tsc --noEmit",
    "test": "jest --collectCoverage",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test --config=playwright/playwright.config.ts --project=chromium",
    "test:e2e:firefox": "playwright test --config=playwright/playwright.config.ts --project=firefox",
    "test:e2e:webkit": "playwright test --config=playwright/playwright.config.ts --project=webkit",
    "test:e2e:smoke": "npm run test:e2e -- --grep @smoke",
    "test:e2e:data": "npm run test:e2e -- --grep @data",
    "test:e2e:report": "playwright show-report",
    "hasura": "env-cmd -f ./hasura/.env hasura --project hasura --skip-update-check",
    "hasura:console": "npm run hasura -- console",
    "hasura:squash": "npm run hasura -- migrate squash",
    "hasura:migrate:all:up": "npm run hasura -- migrate apply --database-name default",
    "hasura:migrate:all:down": "npm run hasura -- migrate apply --type down --version 101 --database-name default",
    "hasura:seed:create": "npm run hasura -- seed create",
    "hasura:seed:apply": "npm run hasura -- seed apply --database-name default",
    "hasura:metadata:apply": "npm run hasura -- metadata apply",
    "hasura:migrate:apply": "npm run hasura -- migrate apply",
    "prepare": "husky install",
    "graphql-codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@googlemaps/react-wrapper": "^1.1.32",
    "@graphql-codegen/cli": "^2.6.2",
    "@hookform/resolvers": "^2.8.8",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.80",
    "@mui/material": "^5.6.4",
    "@mui/utils": "^5.4.2",
    "@mui/x-date-pickers": "^5.0.0-alpha.2",
    "@react-pdf/renderer": "^2.1.1",
    "@stripe/react-stripe-js": "1.7.2",
    "@stripe/stripe-js": "1.29.0",
    "@types/amazon-cognito-auth-js": "^1.3.0",
    "@types/chance": "1.1.3",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-auth": "^0.1.7",
    "amazon-cognito-identity-js": "^5.2.8",
    "autosuggest-highlight": "^3.2.1",
    "aws-amplify": "^4.3.21",
    "chance": "1.1.8",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "deepmerge-ts": "^4.0.1",
    "file-saver": "^2.0.5",
    "graphql": "^16.4.0",
    "graphql-request": "^4.2.0",
    "i18next": "^21.8.0",
    "jszip": "^3.9.1",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "material-ui-phone-number": "^3.0.0",
    "mui-image": "^1.0.4",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.30.0",
    "react-i18next": "^11.16.9",
    "react-otp-input-rc-17": "^2.4.1-minor",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.3.2",
    "react-youtube": "^9.0.2",
    "sanitize-html": "^2.7.0",
    "string-strip-html": "^8.3.0",
    "swr": "^1.3.0",
    "urql": "^2.2.0",
    "use-debounce": "^7.0.1",
    "uuid": "^8.3.2",
    "xlsx": "^0.18.5",
    "yup": "^0.32.11",
    "yup-password": "0.2.2"
  },
