    "hasura:metadata:ls": "pnpm hasura md ic list",
    "hasura:metadata:status": "pnpm hasura md ic status",
    "hasura:migrate:status": "pnpm hasura migrate status",
    "hasura:metadata:apply": "pnpm hasura md apply",
    "hasura:metadata:reload": "pnpm hasura md reload",
    "hasura:migrate:apply": "pnpm hasura migrate apply",
    "hasura:migrate:all:up": "pnpm hasura migrate apply --database-name default",
    "hasura:migrate:all:down": "pnpm hasura migrate apply --type down --version 101 --database-name default",
    "hasura:migrate:down": "pnpm hasura migrate apply --down",
    "hasura:migrate:down:au": "pnpm hasura migrate apply --endpoint http://localhost:8081 --down",
    "hasura:squash": "pnpm hasura migrate squash",
    "hasura:seed:apply": "pnpm hasura seed apply --database-name default",
    "hasura:console:au": "pnpm hasura console --endpoint http://localhost:8081 --console-port 9696",
    "hasura:seed:apply:au": "pnpm hasura seed apply --database-name default --endpoint http://localhost:8081",
    "hasura:migrate:apply:au": "pnpm hasura migrate apply --endpoint http://localhost:8081",
    "hasura:metadata:apply:au": "pnpm hasura md apply --endpoint http://localhost:8081",
    "hasura:metadata:status:au": "pnpm hasura md ic status --endpoint http://localhost:8081",
    "hasura:migrate:status:au": "pnpm hasura migrate status --endpoint http://localhost:8081",
    "hasura:metadata:apply:both": "pnpm hasura:metadata:apply && pnpm hasura:metadata:apply:au",
    "hasura:seed:apply:both": "pnpm hasura:seed:apply && pnpm hasura:seed:apply:au",
    "hasura:migrate:apply:both": "pnpm hasura:migrate:apply && pnpm hasura:migrate:apply:au",
