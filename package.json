    "@types/autosuggest-highlight": "^3.2.0",
    "@types/file-saver": "^2.0.5",
    "@types/google.maps": "^3.48.7",
    "@types/jest": "^27.5.0",
    "@types/lodash-es": "^4.17.6",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "^17.0.45",
    "@types/react-beautiful-dnd": "^13.1.2",
