{
  "name": "@teamteach/hub.web-e2e",
  "version": "1.0.0",
  "private": true,
  "description": "Hub application web portal's E2E tests",
  "authors": [
    "NearForm Ltd.",
    "Amdaris Ltd."
  ],
  "license": "LICENSE.md",
  "scripts": {
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --ui --ui-port=6007 --debug --project=chromium",
    "e2e:queries": "cross-env QUERY=true playwright test --project=queries",
    "e2e:chromium": "playwright test --project=chromium",
    "e2e:firefox": "playwright test --project=firefox",
    "e2e:webkit": "playwright test --project=webkit",
    "e2e:smoke": "playwright test --project=chromium --grep @smoke",
    "e2e:report": "playwright show-report",
    "e2e:sandbox": "playwright codegen",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.snap",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.snap",
    "graphql-codegen": "graphql-codegen -c ./codegen.yml"
  },
  "dependencies": {},
  "devDependencies": {},
  "nx": {}
}
